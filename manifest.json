{
    "name":"SC Helper 3",
    "short_name":"schelper3",
    "description":"SC Scripts",
    "version":"3.0.0",
    "homepage_url":"https://github.com/risonakamo/schelper3",
    "manifest_version":3,
    "author":"risonakamo",

    "permissions": [
        "tabs",
        "scripting"
    ],

    "host_permissions":[
        "https://exhentai.org/s/*",
        "https://chan.sankakucomplex.com/en/posts/*",
        "https://chan.sankakucomplex.com/posts/*"
    ],

    "action":{
        "default_title":"SC Helper 3",
        "default_popup":"build/popup.html"
    },

    "content_scripts": [{
        "matches":[
            "https://exhentai.org/g/*"
        ],
        "js":["build-cs/schelper.iife.js"],
        "run_at":"document_end"
    }]
}